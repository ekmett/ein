{
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "ctest-lab.buildDirectory": "gen",
  "files.exclude": {
    "!.ccache/ccache.conf": true,
    ".ccache/?/": true,
    ".ccache/lock": true,
    ".ccache/tmp": true,
    "gen/**": true,
    "lib/cache/**": true,
    "old/**": true,
    "Testing/**": true
  },
  "files.watcherExclude": {
    "gen/**": true,
    "lib/cache/**": true
  },
  "explorer.excludeGitIgnore": true
}
