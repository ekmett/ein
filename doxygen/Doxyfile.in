# Minimal Doxyfile.in template

PROJECT_NAME = @PROJECT_NAME@
PROJECT_NUMBER = @PROJECT_VERSION@
ENABLE_PREPROCESSING = YES
EXCLUDE += src/ein/map.hpp
EXPAND_ONLY_PREDEF = NO
MACRO_EXPANSION = YES
EXTENSION_MAPPING = cppm=C++
EXTRACT_ALL = YES
EXTRACT_PRIVATE = YES
EXTRACT_STATIC = YES
EXTRACT_LOCAL_METHODS = YES
EXTRACT_LOCAL_CLASSES = YES
EXTRACT_ANON_NSPACES = NO
FILE_PATTERNS = *.h *.hpp *.c *.cpp *.cppm
GENERATE_HTML = YES
GENERATE_LATEX = NO
GROUP_NESTED_COMPOUNDS = YES
INPUT = @CMAKE_SOURCE_DIR@/README.md @CMAKE_SOURCE_DIR@/src
MACRO_EXPANSION = YES
OUTPUT_DIRECTORY = @CMAKE_BINARY_DIR@/docs
PREDEFINED = DOXYGEN=1 __cpp_modules=201907L EIN(x)= ein_inline= __AVX512F__=1 __AVX2__=1 ein_artificial=
PROJECT_NUMBER = "@PROJECT_VERSION@"
QUIET = YES
RECURSIVE = YES
WARN_IF_DOC_ERROR = YES
WARN_IF_UNDOCUMENTED = YES
USE_MDFILE_AS_MAINPAGE = @CMAKE_SOURCE_DIR@/README.md
INTERNAL_DOCS = NO
AUTOLINK_SUPPORT       = YES
DISTRIBUTE_GROUP_DOC = YES
INLINE_GROUPED_CLASSES = YES
CASE_SENSE_NAMES = NO

GENERATE_TODOLIST      = YES
GENERATE_TREEVIEW      = YES
DISABLE_INDEX          = NO
FULL_SIDEBAR           = NO
HTML_COLORSTYLE        = LIGHT
HTML_EXTRA_STYLESHEET  = @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome.css
HTML_EXTRA_STYLESHEET += @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome-sidebar-only-darkmode-toggle.css
HTML_EXTRA_STYLESHEET += @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome-sidebar-only.css
HTML_EXTRA_STYLESHEET += @CMAKE_SOURCE_DIR@/doxygen/header.css
HTML_EXTRA_FILES       = @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome-darkmode-toggle.js
HTML_EXTRA_FILES      += @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome-fragment-copy-button.js
HTML_EXTRA_FILES      += @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome-paragraph-link.js
HTML_EXTRA_FILES      += @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome-interactive-toc.js
HTML_EXTRA_FILES      += @FETCHCONTENT_BASE_DIR@/doxygen-awesome-css-src/doxygen-awesome-tabs.js
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/ein-logo.png
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/favicon.ico
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/favicon-16x16.png
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/favicon-32x32.png
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/android-chrome-192x192.png
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/android-chrome-512x512.png
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/apple-touch-icon.png
HTML_EXTRA_FILES      += @CMAKE_SOURCE_DIR@/doxygen/site.webmanifest
HTML_HEADER            = @CMAKE_SOURCE_DIR@/doxygen/header.html
HTML_COPY_CLIPBOARD    = NO
PROJECT_LOGO           = @CMAKE_SOURCE_DIR@/doxygen/ein-logo.png
PROJECT_ICON           = @CMAKE_SOURCE_DIR@/doxygen/favicon.ico
CREATE_SUBDIRS         = YES
MARKDOWN_SUPPORT       = YES
TOC_INCLUDE_HEADINGS   = 3
HAVE_DOT = YES
DOT_IMAGE_FORMAT = svg
DOT_GRAPH_MAX_NODES = 100
INLINE_SIMPLE_STRUCTS = YES
USE_MATHJAX = YES
MATHJAX_RELPATH = @CMAKE_SOURCE_DIR@/doxygen/mathjax.js
MATHJAX_VERSION = MathJax_3
MATHJAX_EXTENSIONS = ams
SOURCE_BROWSER = YES
INLINE_SOURCES = YES
CALLER_GRAPH = YES

