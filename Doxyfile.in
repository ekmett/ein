# Minimal Doxyfile.in template

PROJECT_NAME = @PROJECT_NAME@
PROJECT_NUMBER = @PROJECT_VERSION@
ENABLE_PREPROCESSING = YES
EXCLUDE += src/ein/map.hpp
EXPAND_ONLY_PREDEF = NO
EXTENSION_MAPPING = cppm=C++
EXTRACT_ALL = YES
EXTRACT_PRIVATE = YES
EXTRACT_STATIC = YES
FILE_PATTERNS = *.h *.hpp *.c *.cpp *.cppm
GENERATE_HTML = YES
GENERATE_LATEX = NO
GROUP_NESTED_COMPOUNDS = YES
INPUT = @CMAKE_SOURCE_DIR@/README.md @CMAKE_SOURCE_DIR@/src
MACRO_EXPANSION = YES
OUTPUT_DIRECTORY = @CMAKE_BINARY_DIR@/docs
PREDEFINED = DOXYGEN=1 __cpp_modules=201907L EIN(x)=
PROJECT_NUMBER = "@PROJECT_VERSION@"
QUIET = YES
RECURSIVE = YES
WARN_IF_DOC_ERROR = YES
WARN_IF_UNDOCUMENTED = YES
USE_MDFILE_AS_MAINPAGE = @CMAKE_SOURCE_DIR@/README.md
INTERNAL_DOCS = NO
